<% @page_title = browse_page_title letter: @letter, browse_by: 'Jesuits' %>

<h2>Browse all Jesuits</h2>

<%= alphabetical_paginate @alpha_params %>

<div id="pagination_table" class="term-list-container">
  <% @entities.each do |entity| %>
    <div class="term-list-item"><a href="/catalog?f[jesuits_facet][]=<%= entity.display_name %>"><%= entity.display_name %></a></div>
  <% end %>
</div>